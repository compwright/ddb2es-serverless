{
  "name": "@compwright/ddb2es-serverless",
  "version": "2.0.0",
  "description": "Serverless handler to stream AWS DynamoDB to Elasticsearch (forked from dynamo2es-lambda)",
  "repository": {
    "type": "git",
    "url": "https://github.com/compwright/dynamo2es-lambda.git"
  },
  "keywords": [
    "amazon",
    "aws",
    "aws-es",
    "@elastic",
    "elasticsearch",
    "elasticsearch-js",
    "client",
    "ddb",
    "dynamo",
    "dynamodb",
    "dynamodb-streams",
    "lambda",
    "serverless",
    "streams",
    "dynamo2es-lambda"
  ],
  "main": "src/",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "pretest": "standard --fix",
    "test": "nyc mocha --opts ./mocha.opts ./test"
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**"
    ],
    "exclude": [
      "**/test/**"
    ],
    "excludeNodeModules": true,
    "reporter": [
      "lcov",
      "text-summary"
    ]
  },
  "author": {
    "name": "Jonathon Hill",
    "email": "jhill9693@gmail.com"
  },
  "contributors": [
    {
      "name": "Anton Bazhal",
      "email": "am.bazhal@gmail.com"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "@hapi/joi": "^15.1.0",
    "lodash": "^4.17.15",
    "p-retry": "^4.1.0"
  },
  "peerDependencies": {
    "@elastic/elasticsearch": "^7.3.0",
    "aws-sdk": "^2.514.0"
  },
  "devDependencies": {
    "aws-sdk": "^2.514.0",
    "chai": "^4.2.0",
    "chai-subset": "^1.6.0",
    "elasticsearch": "^16.3.0",
    "env-test": "^1.0.0",
    "lambda-tester": "^3.6.0",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "sinon": "^7.4.1",
    "standard": "^14.1.0",
    "uuid": "^3.3.2"
  }
}
